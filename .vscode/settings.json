{
    "typescript.tsdk": "node_modules/typescript/lib",
    "files.exclude": {
      "**/.git": true,
      "**/.DS_Store": true,
      "node_modules": true,
      "coverage": true,
      ".nyc_output": true,
      "**/*.js": {
        "when": "$(basename).ts"
      },
      "{src,test/conformance}/**/*.d.ts": true,
      "**/*.js.map": true,
      "test/{integration,rules}/**/*.{fix,lint}": true,
    }
}
